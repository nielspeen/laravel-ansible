{
    "name": "nielspeen/laravel-ansible",
    "description": "A Laravel wrapper for Ansible.",
    "keywords": [
        "laravel",
        "ansible"
    ],
    "homepage": "https://github.com/nielspeen/laravel-ansible",
    "type": "library",
    "license": "MIT",
    "authors": [
	{
		"name": "Niels Peen",
		"email": "niels@peen.email"
	},
        {
            "name": "Marc Aschmann",
            "email": "maschmann@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1.0",
        "psr/log": "^3.0",
        "symfony/process": "^5.3|^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10",
        "mikey179/vfsstream": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "Peen\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Peen\\": "tests/Peen"
        }
    },
    "scripts": {
        "test": [
            "phpunit --configuration phpunit.xml.dist"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Peen\\Ansible\\AnsibleServiceProvider"
            ],
            "aliases": {
                "AnsiblePlaybook": "Peen\\Ansible\\Facades\\AnsiblePlaybook",
                "AnsibleGalaxy": "Peen\\Ansible\\Facades\\AnsibleGalaxy"
            }
        }
    }
}
